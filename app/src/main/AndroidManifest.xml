<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="babiy.reminder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Monday_Activity" />
        <activity android:name=".Tuesday_Activity" />
        <activity android:name=".Wednesday_Activity" />
        <activity android:name=".Thursday_Activity" />
        <activity android:name=".Friday_Activity" />
        <activity android:name=".Saturday_Activity" />
        <activity android:name=".Sunday_Activity"
            android:label="Sunday"/>
        <activity android:name=".Edit_Activity"></activity>
    </application>

</manifest>